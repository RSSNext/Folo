      "types": "./src/collection/*.ts",
      "require": "./src/collection/*.ts",
      "import": "./src/collection/*.ts"
    },
    "./constants/*": {
      "types": "./src/constants/*.ts",
      "require": "./src/constants/*.ts",
      "import": "./src/constants/*.ts"
    },
    "./context": {
      "types": "./src/context.ts",
      "require": "./src/context.ts",
      "import": "./src/context.ts"
    },
    "./entry/*": {
      "types": "./src/entry/*.ts",
      "require": "./src/entry/*.ts",
      "import": "./src/entry/*.ts"
