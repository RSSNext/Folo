      "types": "./src/morph/*.ts",
      "require": "./src/morph/*.ts",
      "import": "./src/morph/*.ts"
    },
    "./subscription/*": {
      "types": "./src/subscription/*.ts",
      "require": "./src/subscription/*.ts",
      "import": "./src/subscription/*.ts"
    },
    "./summary/*": {
      "types": "./src/summary/*.ts",
      "require": "./src/summary/*.ts",
      "import": "./src/summary/*.ts"
    },
    "./translation/*": {
      "types": "./src/translation/*.ts",
      "require": "./src/translation/*.ts",
      "import": "./src/translation/*.ts"
    },
    "./unread/*": {
      "types": "./src/unread/*.ts",
      "require": "./src/unread/*.ts",
      "import": "./src/unread/*.ts"
    },
    "./user/*": {
      "types": "./src/user/*.ts",
      "require": "./src/user/*.ts",
      "import": "./src/user/*.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "19.0.0"
  },
  "dependencies": {
    "@follow/configs": "workspace:*",
    "@follow/constants": "workspace:*",
    "@follow/database": "workspace:*",
    "@follow/models": "workspace:*",
    "@follow/shared": "workspace:*",
    "@follow/tracker": "workspace:*",
    "@follow/utils": "workspace:*",
    "@tanstack/react-query": "5.77.2",
